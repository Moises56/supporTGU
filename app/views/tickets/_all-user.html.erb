<div class="container pt-4">
  <div class="row categorias-class" id="categorias">
    <% @user.each do |user| %>
      <div class="col-12 col-md-4 col-6 col-sm-8 mt-1">
        <div class="card card-color">
            <span>ID: <%= user.id%></span>
            <div class="card-body">
              <h5 class="card-title title-email"><%= user.email%></h5>
              <p class="card-text">Fecha: <%= user.created_at%></p>
            </div>
        </div>
         <td>
        </td>
      </div> 
      <%# <%=button_to 'delete', @user, method: :delete2, data: {confirm: "Deseas eliminar este ticket?", disable_with: "Processing..."}, class: 'btn btn btn-outline-danger btn-xs' %> 
    <% end %>
    

  
  </div>
</div>

